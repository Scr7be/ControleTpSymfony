{% extends 'base.html.twig' %}

{% block title %}Création d'un contact'{% endblock %}


{% block body %}
<div class="container">
        <h1>Créer un nouveau contact !</h1>

        {{ form_start(form) }}

        {{ form_widget(form) }}

        <button type="submit" class="btn btn-primary">Créer le contact</button>

        {{ form_end(form) }}

        <a href='{{path('contact_liste')}}' class='btn btn-primary'>Annuler</a>
</div>

{% endblock %}